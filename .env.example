# MongoDB credentials (optional - defaults in docker-compose work for dev)
# Copy to .env and set your own values for production.
MONGO_INITDB_ROOT_PASSWORD=adminPass
MONGO_APP_USER_PASSWORD=portfolioDbPass

# PostgreSQL credentials (optional - defaults in docker-compose work for dev)
# postgres = admin user (like Mongo adminUser)
POSTGRES_ROOT_PASSWORD=adminPass
POSTGRES_APP_PASSWORD=portfolioDbPass
